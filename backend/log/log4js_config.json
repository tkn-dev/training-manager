{
  "appenders": {
    "system": {
      "type": "file",
      "filename": "/var/log/trm_system.log",
      "backups": 7,
      "pattern": "-yyyy-MM-dd"
    },
    "console": {
      "type": "stdout"
    }
  },
  "categories": {
    "default": {
      "appenders": ["system", "console"],
      "level": "mark"
    }
  }
}
